#include<iostream>
using namespace std;

main()
{  int n,a=0;
 
  cout<<"Digite un numero: "<<endl;
  cin>>n;
  for  (int i=1;i<n;i++)
       {
         if (n%i==0)
            {
              a=a+i;
             }
      
       }
     
  if (a<n)
    {
       cout<<"El numero es Deficiente"<<endl<<endl;
     }   
   else
      cout<<"El numero no es Deficiente"<<endl<<endl;

system("pause");
return 0;

}
